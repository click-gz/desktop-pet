# AI Service Configuration (choose one or configure both for failover)

# Option 1: OpenAI API
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
# OPENAI_BASE_URL=https://api.openai.com/v1

# Option 2: SiliconFlow API (Recommended for China)
SILICONFLOW_API_KEY=your_siliconflow_token_here
SILICONFLOW_MODEL=Qwen/QwQ-32B
SILICONFLOW_BASE_URL=https://api.siliconflow.cn/v1

# AI Provider Priority (optional, default: siliconflow,openai)
# The system will try services in this order
AI_PROVIDER_PRIORITY=siliconflow,openai

# Server Configuration
PORT=3000
HOST=0.0.0.0

# Redis Configuration (User Profile System)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

